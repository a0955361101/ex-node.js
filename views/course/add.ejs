<%- include('../parts/header') %> <%- include('../parts/navbar') %>

<div class="container">
    <div class="row">
        <div class="col-lg-6">
            <h3>新增資料</h3>
            <form name="form1">
                <div class="mb-3">
                    <label for="" class="form-label">name</label>
                    <input
                        type="text"
                        class="form-control"
                        name="course_name"
                    />
                    <div class="form-text"></div>
                </div>
                <div class="mb-3">
                    <label for="" class="form-label">price</label>
                    <input
                        type="text"
                        class="form-control"
                        name="course_price"
                    />
                    <div class="form-text"></div>
                </div>
                <div class="mb-3">
                    <label for="" class="form-label">content</label>
                    <input
                        type="text"
                        class="form-control"
                        name="course_content"
                    />
                    <div class="form-text"></div>
                </div>
                <div class="mb-3">
                    <label for="" class="form-label">people</label>
                    <input
                        type="text"
                        class="form-control"
                        name="course_people"
                    />
                    <div class="form-text"></div>
                </div>
                <div class="mb-3">
                    <label for="" class="form-label">material</label>
                    <input
                        type="text"
                        class="form-control"
                        name="course_material"
                    />
                    <div class="form-text"></div>
                </div>

                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>

<%- include('../parts/script') %> <%- include('../parts/footer') %>
